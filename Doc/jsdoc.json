{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src/"],
        "exclude": ["./node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./Doc/",
        "recurse": true
    }
}